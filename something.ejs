<% campgrounds.forEach(function(camp){ %>
                    <div class="col-md-3" > 
                        <div class="well" style=" padding:0"> 
                            <img class="mygridImg" src=" <%= camp.image %>" />
                            <div class="caption">
                                <h3><%= camp.name %> </h3> 
                            </div>
                        </div>
                    </div>
                <% }) %>